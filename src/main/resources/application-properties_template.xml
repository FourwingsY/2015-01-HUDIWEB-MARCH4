<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!--db 설정 템플릿 파일-->
<properties>
	<comment>application-properties</comment>
	
	<entry key="database.driverClassName">com.mysql.jdbc.Driver</entry>
	<entry key="database.url">jdbc:mysql://localhost:3306/<!--myDBName--></entry>
	<entry key="database.username"><!--userName--></entry>
	<entry key="database.password"><!--password--></entry>
</properties>